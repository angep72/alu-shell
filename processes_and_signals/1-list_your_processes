#!/usr/bin/env bash
#This script displays the list of running processes
# Get the process information
process_info=$(ps -eo user,lstart,command | grep "7-highlander" | grep -v grep)

# Check if the process is running
if [ -z "$process_info" ]; then
  echo "Process is not running."
else
  # Extract the relevant information from the process info
  user=$(echo "$process_info" | awk '{print $1}')
  start_time=$(echo "$process_info" | awk '{print $2" "$3" "$4}')
  command=$(echo "$process_info" | awk '{$1=$2=$3=""; print $0}')

  # Print the process information
  echo "USER STARTED COMMAND"
  echo "$user $start_time $command"
fi

