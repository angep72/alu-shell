#!/usr/bin/env bash
#tis is for question 
#!/bin/bash

current_pid=$$

for pid in /proc/*/; do
    pid=${pid%/}
    pid=${pid##*/}
    if [[ -f "/proc/$pid/cmdline" ]]; then
        cmdline=$(tr '\0' ' ' < "/proc/$pid/cmdline")
        if [[ "$cmdline" == */bin/bash* && "$pid" != "$current_pid" ]]; then
            echo "$pid $cmdline"
        fi
    fi
done
