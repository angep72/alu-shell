#!/usr/bin/env bash
# Get the current script's PID
script_pid=$$

# Get the process IDs of all processes containing the word "bash" in their name
pids=$(pgrep -f bash)

# Iterate over the process IDs and display the PID and process name
for pid in $pids; do
    # Exclude the current script's PID from the output
    if [[ $pid -ne $script_pid ]]; then
        # Get the process name using the proc file system
        process_name=$(cat "/proc/$pid/cmdline" | tr '\0' ' ')
        echo "$pid $process_name"
    fi
done
